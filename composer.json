{
    "name": "beephp/beephp",
    "description": "A lightweight PHP micro framework for building efficient web applications.",
    "keywords": ["framework", "php", "micro"],
    "license": "MIT",
    "homepage": "https://beephp.ir",
    "support": {
        "issues": "https://github.com/beephp/beephp/issues",
        "source": "https://github.com/beephp/beephp"
    },
    "authors": [
        {
            "name": "Usermp",
            "email": "usermp76@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "beephp/framework": "dev-master"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {},
    "suggest": {},
    "type": "project"
}
